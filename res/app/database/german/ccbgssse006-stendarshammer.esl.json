[{"original": "Smite Damage", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "Schmetterschaden"}, {"original": "Damages the target for <mag> points. Depletes <20> Stamina from wielder per hit.", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "Fügt dem Ziel <mag> Schaden zu. Jeder Schlag kostet den Träger <20> Ausdauer."}, {"original": "Reduced Stamina", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_DamageStamina", "string": "Ausdauer verringert"}, {"original": "Drains <mag> points from stamina.", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_DamageStamina", "string": "Entzieht <mag> Punkte Ausdauer."}, {"original": "Absorb Health", "form_id": "03000807|ccbgssse006-stendarshammer.esl", "type": "ENCH FULL", "editor_id": "ccBGSSSE006_EnchDrainHealth", "string": "Gesundheit absorbieren"}, {"original": "Smite", "form_id": "0300080A|ccbgssse006-stendarshammer.esl", "type": "SPEL FULL", "editor_id": "ccBGSSSE006_DrainStamina", "string": "Ausdauer entziehen"}, {"original": "Stendarr's Hammer", "form_id": "03000D62|ccbgssse006-stendarshammer.esl", "type": "WEAP FULL", "editor_id": "ccBGSSSE006StendarsHammer", "string": "Stendarrs Hammer"}, {"original": "Dwemer Museum", "form_id": "00030313|Skyrim.esm", "type": "CELL FULL", "editor_id": "MarkarthWizardsQuarters01", "string": "Dwemermuseum"}, {"original": "If I had a Hammer", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST FULL", "editor_id": "ccBGSSSE006_Quest", "string": "Hätte ich nur einen Hammer"}, {"original": "Rumor has it that Stendarr's Hammer has been found deep under Markarth. They say it is in the Dwarven Museum there.", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST CNAM", "index": 95, "editor_id"