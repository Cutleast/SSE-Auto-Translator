[{"original": "Smite Damage", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "Dégâts de châtiment"}, {"original": "Damages the target for <mag> points. Depletes <20> Stamina from wielder per hit.", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "Inflige <mag> points de dégâts à la cible. Réduit la vigueur du porteur de <20> points à chaque coup."}, {"original": "Reduced Stamina", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_DamageStamina", "string": "Vigueur dégradée"}, {"original": "Drains <mag> points from stamina.", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_DamageStamina", "string": "Ponctionne <mag> points de vigueur."}, {"original": "Absorb Health", "form_id": "03000807|ccbgssse006-stendarshammer.esl", "type": "ENCH FULL", "editor_id": "ccBGSSSE006_EnchDrainHealth", "string": "Siphon de santé"}, {"original": "Smite", "form_id": "0300080A|ccbgssse006-stendarshammer.esl", "type": "SPEL FULL", "editor_id": "ccBGSSSE006_DrainStamina", "string": "Ponction de vigueur"}, {"original": "Stendarr's Hammer", "form_id": "03000D62|ccbgssse006-stendarshammer.esl", "type": "WEAP FULL", "editor_id": "ccBGSSSE006StendarsHammer", "string": "Marteau de Stendarr"}, {"original": "Dwemer Museum", "form_id": "00030313|Skyrim.esm", "type": "CELL FULL", "editor_id": "MarkarthWizardsQuarters01", "string": "Musée des Dwemers"}, {"original": "If I had a Hammer", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST FULL", "editor_id": "ccBGSSSE006_Quest", "string": "Si j'avais un marteau"}, {"original": "Rumor has it that Stendarr's Hammer has been found deep under Markarth. They say it is in the Dwarven Museum there.", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST CNAM", "index": 95, "editor_id": "ccBGSSSE006_Quest", "string": "La rumeur indique que le Marteau de Stendarr a été trouvé dans les profondeurs du sol qui se trouve sous Markath. Il paraît qu'il est exposé au musée des Nains de la ville."}, {"original": "I have successfully stolen Stendarr's Hammer from the Dwarven Museum in Markarth.", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST CNAM", "index": 96, "editor_id": "ccBGSSSE006_Quest", "string": "J'ai réussi à dérober le Marteau de Stendarr au musée des Nains de Markath."}, {"original": "Steal Stendarr's Hammer", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST NNAM", "index": 0, "editor_id": "ccBGSSSE006_Quest", "string": "Dérobez le Marteau de Stendarr"}]