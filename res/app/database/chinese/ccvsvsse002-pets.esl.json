[{"original": "Fox", "form_id": "0500080E|ccvsvsse002-pets.esl", "type": "RACE FULL", "editor_id": "ccVSVSSE002_FoxRace", "string": "狐狸"}, {"original": "Rabbit", "form_id": "0500080F|ccvsvsse002-pets.esl", "type": "RACE FULL", "editor_id": "ccVSVSSE002_RabbitRace", "string": "兔子"}, {"original": "Skeever", "form_id": "05000810|ccvsvsse002-pets.esl", "type": "RACE FULL", "editor_id": "ccVSVSSE002_SkeeverRace", "string": "惡鼠"}, {"original": "Frostbite Spider", "form_id": "05000811|ccvsvsse002-pets.esl", "type": "RACE FULL", "editor_id": "ccVSVSSE002_SpiderRace", "string": "霜噬蜘蛛"}, {"original": "Goat", "form_id": "05000812|ccvsvsse002-pets.esl", "type": "RACE FULL", "editor_id": "ccVSVSSE002_GoatRace", "string": "山羊"}, {"original": "Command Spider", "form_id": "05000817|ccvsvsse002-pets.esl", "type": "MGEF FULL", "editor_id": "ccVSVSSE002_MGEF_CommandSpider", "string": "指揮蜘蛛"}, {"original": "Command your pet spider to spin a web trap at the target location. Only <1> web trap can be active at a time.", "form_id": "05000817|ccvsvsse002-pets.esl", "type": "MGEF DNAM", "editor_id": "ccVSVSSE002_MGEF_CommandSpider", "string": "指揮蜘蛛在目標位置織網作為陷阱。一次只能使用<1>個蛛網陷阱。"}, {"original": "Stagger Push", "form_id": "05000818|ccvsvsse002-pets.esl", "type": "MGEF FULL", "editor_id": "ccVSVSSE002_MGEF_StaggerCont", "string": "失衡推擊"}, {"original": "Teleport Arachnia", "form_id": "05000918|ccvsvsse002-pets.esl", "type": "MGEF FULL", "editor_id": "ccVSVSSE002_MGEF_TeleportSpider", "string": "傳送阿拉克尼亞"}, {"original": "Teleports Arachnia the spider", "form_id": "05000918|ccvsvsse002-pets.esl", "type": "MGEF DNAM", "editor_id": "ccVSVSSE002_MGEF_TeleportSpider", "string": "傳送蜘蛛阿拉克尼亞"}, {"original": "Teleport Skritch", "form_id": "05000919|ccvsvsse002-pets.esl", "type": "MGEF FULL", "editor_id": "ccVSVSSE002_MGEF_TeleportSkeever", "string": "傳送斯克里奇"}, {"original": "Teleports Skritch the skeever", "form_id": "05000919|ccvsvsse002-pets.esl", "type": "MGEF DNAM", "editor_id": "ccVSVSSE002_MGEF_TeleportSkeever", "string": "傳送惡鼠牙牙"}, {"original": "Teleport Hilda", "form_id": "0500091A|ccvsvsse002-pets.esl", "type": "MGEF FULL", "editor_id": "ccVSVSSE002_MGEF_TeleportGoat", "string": "傳送希爾達"}, {"original": "Teleports Hilda the goat", "form_id": "0500091A|ccvsvsse002-pets.esl", "type": "MGEF DNAM", "editor_id": "ccVSVSSE002_MGEF_TeleportGoat", "string": "傳送山羊希爾達"}, {"original": "Teleport Sweet Roll", "form_id": "0500091B|ccvsvsse002-pets.esl", "type": "MGEF FULL", "editor_id": "ccVSVSSE002_MGEF_TeleportFox", "string": "傳送小甜捲"}, {"original": "Teleports Sweet Roll the fox", "form_id": "0500091B|ccvsvsse002-pets.esl", "type": "MGEF DNAM", "editor_id": "ccVSVSSE002_MGEF_TeleportFox", "string": "傳送狐狸小甜捲"}, {"original": "Teleport Thistle", "form_id": "0500091C|ccvsvsse002-pets.esl", "type": "MGEF FULL", "editor_id": "ccVSVSSE002_MGEF_TeleportRabbit", "string": "傳送小薊"}, {"original": "Teleports Thistle the rabbit", "form_id": "0500091C|ccvsvsse002-pets.esl", "type": "MGEF DNAM", "editor_id": "ccVSVSSE002_MGEF_TeleportRabbit", "string": "傳送兔子小薊"}, {"original": "Fortify Carry Weight", "form_id": "0500093C|ccvsvsse002-pets.esl", "type": "MGEF FULL", "editor_id": "ccVSVSSE002_EnchFortifyCarryConstantSelf", "string": "負重提升<mag>點。"}, {"original": "Carrying capacity increased by <mag> points.", "form_id": "0500093C|ccvsvsse002-pets.esl", "type": "MGEF DNAM", "editor_id": "ccVSVSSE002_EnchFortifyCarryConstantSelf", "string": "強化負重"}, {"original": "Paralyze", "form_id": "05000816|ccvsvsse002-pets.esl", "type": "MGEF FULL", "editor_id": "ccVSVSSE002_MGEF_WebTrap", "string": "麻痺"}, {"original": "Fortify Carry Weight", "form_id": "0500093B|ccvsvsse002-pets.esl", "type": "ENCH FULL", "editor_id": "ccVSVSSE002_ENCH_FortifyCarry", "string": "負重增強"}, {"original": "Teleport Pet: Thistle", "form_id": "0500091E|ccvsvsse002-pets.esl", "type": "SPEL FULL", "editor_id": "ccVSVSSE002_SPELL_TeleportRabbit", "string": "傳送寵物：小薊"}, {"original": "Teleport Pet: Hilda", "form_id": "0500091F|ccvsvsse002-pets.esl", "type": "SPEL FULL", "editor_id": "ccVSVSSE002_SPELL_TeleportGoat", "string": "傳送寵物：希爾達"}, {"original": "Teleport Pet: Sweet Roll", "form_id": "05000920|ccvsvsse002-pets.esl", "type": "SPEL FULL", "editor_id": "ccVSVSSE002_SPELL_TeleportFox", "string": "傳送寵物：小甜捲"}, {"original": "Teleport Pet: Arachnia", "form_id": "05000921|ccvsvsse002-pets.esl", "type": "SPEL FULL", "editor_id": "ccVSVSSE002_SPELL_TeleportSpider", "string": "傳送寵物：阿拉克尼亞"}, {"original": "Command Spider", "form_id": "05000819|ccvsvsse002-pets.esl", "type": "SPEL FULL", "editor_id": "ccVSVSSE002_SPELL_CommandSpider", "string": "指揮蜘蛛"}, {"original": "Carry Weight Boost", "form_id": "0500093D|ccvsvsse002-pets.esl", "type": "SPEL FULL", "editor_id": "ccVSVSSE002_AB_CarryWeightBoost", "string": "Carry Weight Boost"}, {"original": "Web Trap", "form_id": "0500081A|ccvsvsse002-pets.esl", "type": "SPEL FULL", "editor_id": "ccVSVSSE002_SPELL_WebTrap", "string": "蛛網陷阱"}, {"original": "Teleport Pet: Skritch", "form_id": "0500091D|ccvsvsse002-pets.esl", "type": "SPEL FULL", "editor_id": "ccVSVSSE002_SPELL_TeleportSkeever", "string": "傳送寵物：斯克里奇"}, {"original": "Rabbit", "form_id": "05000820|ccvsvsse002-pets.esl", "type": "ARMO FULL", "editor_id": "ccVSVSSE002_SkinRabbit", "string": "兔子"}, {"original": "Animal Collar", "form_id": "05000821|ccvsvsse002-pets.esl", "type": "ARMO FULL", "editor_id": "ccVSVSSE002_Collar", "string": "動物項圈"}, {"original": "Goat", "form_id": "05000822|ccvsvsse002-pets.esl", "type": "ARMO FULL", "editor_id": "ccVSVSSE002_SkinGoat", "string": "山羊"}, {"original": "Animal Pack", "form_id": "0500081C|ccvsvsse002-pets.esl", "type": "ARMO FULL", "editor_id": "ccVSVSSE002_SaddleBags", "string": "動物背包"}, {"original": "Equip on a pet goat or skeever to increase its carryweight by 40.", "form_id": "0500081C|ccvsvsse002-pets.esl", "type": "ARMO DESC", "editor_id": "ccVSVSSE002_SaddleBags", "string": "裝備在寵物山羊或惡鼠身上，可使其負重提升40。"}, {"original": "Halvar's Journal", "form_id": "05000827|ccvsvsse002-pets.esl", "type": "BOOK FULL", "editor_id": "ccVSVSSE002_Instructions_Goat", "string": "哈瓦爾的日記"}, {"original": "<font face='$HandwrittenFont'>\r\nCame into town a few days ago looking for work. The folks here are nice enough, and there's plenty of wheat and potatoes needing to be harvested, so I've got a steady supply of coin coming in. \r\n\r\nI don't think I'm going to need my old pack goat Hilda for much longer...all my bones are starting to creak, and my adventuring days are long over. Now I just need to find someone willing to take care of her. Maybe get a few coins for her too - there's strength in those old bones of hers yet.\r\n\r\nAh, how I'm going to miss wandering the wilds of Skyrim! I saw so many strange and wonderful things out there - pine trees as tall as mountains, old nord tombs, I once even stumbled across a wandering spirit made of pure ice!\r\n\r\nEven ran across a few folks trying their hand at taming different types of creatures. Don't know why they'd want anything but a loyal dog or trusty goat like my Hilda though - some of them beasts out in the wild just aren't meant to be tamed.\r\n\r\nRan across an alchemist with a pet rabbit, and almost got robbed by a thief with a tame fox, but I suppose a rabbit or a fox could be docile enough. But that pickpocket trying to train a skeever must have been as mad as old Sheo'. And I don't want to think too hard about the vampire that almost made a meal out of me...could have sworn it had a trained frostbite spider! Gives me the shivers just thinking about it.", "form_id": "05000827|ccvsvsse002-pets.esl", "type": "BOOK DESC", "editor_id": "ccVSVSSE002_Instructions_Goat", "string": "<font face='$HandwrittenFont'>\r\n我進城找工作已經有幾天時間了。這裡的人都很好，有很多小麥和馬鈴薯需要收割，看來我的收入暫時是穩定下來了。\r\n\r\n我想我不再需要我的老山羊希爾達幫我背東西了......我全身的骨頭都老到嘎吱嘎吱響了，我的冒險老早就結束了。現在我只需要找到願意照顧牠的人，也許還能藉此賺到一點錢。牠可是老當益壯呢。\r\n\r\n啊，我肯定會很懷念以前在天際省野外逍遙的日子！我在那裡看了好多奇妙的事物，有高聳如山的松樹、古老的諾德墳墓。我甚至無意間碰見純冰形成的精魄在遊蕩！\r\n\r\n也遇過試圖馴服不同種類生物的人。不知道他們為什麼不要忠心的狗，或像希爾達那樣值得信賴的山羊，而是想要其他生物。有些野生動物天生就不該被馴服。\r\n\r\n我還遇過帶著寵物兔的鍊金術士，也曾差點被帶著狐狸的小偷搶劫。我想兔子或狐狸都算是很溫馴的動物，但想要訓練惡鼠的扒手肯定就跟老希奧一樣瘋狂。我不想太認真回想那個差點讓我把飯全吐出來的吸血鬼......我敢發誓他訓練的肯定是霜噬蜘蛛！光用想的就讓我不寒而慄。"}, {"original": "Thief's Journal", "form_id": "05000828|ccvsvsse002-pets.esl", "type": "BOOK FULL", "editor_id": "ccVSVSSE002_Instructions_Fox", "string": "小偷的日記"}, {"original": "<font face='$HandwrittenFont'><div align=\"left\">\r\n<b>4th of First Seed</b>\r\nI guess being Bosmer really does give me a way with animals! This little fox wandered up to me today, bold as brass, and started begging for treats. I didn't have anything on me at the time but a sweet roll, but it seemed to like that well enough. I wonder if this gift with animals works on horses - it would make stealing them dead easy.\r\n\r\n<b>15th of First Seed</b>\r\nThe furry nuisance won't leave me alone! It's been following me everywhere, even down into the sewers. At least it's quiet enough that it won't alert the guards...\r\n\r\n<b>1st of Rain's Hand</b>\r\nThe fox has a real nose for sniffing out valuables. Just this morning she dug a silver locket out of the mud - I never would have seen it if she hadn't started pawing at the ground. She must be attracted to shiny things. Can't say I blame her, really.\r\n\r\n<b>9th of Rain's Hand</b>\r\nFinding Sweet Roll has turned out to be the best stroke of luck I've had in a while. She's got better eyes and instincts than any thief I've ever met, including Vex. I can't remember the last time my coin purse was this heavy - I'm starting to jingle like a battalion of Imperials when I walk.\r\n\r\n<b>25th of Rain's Hand</b>\r\nI think I'll try out this animal-taming thing on the wolves outside of town. Some of the other thieves have been eyeing me recently; they would think twice about jumping me if I had a wolf at my heels. ", "form_id": "05000828|ccvsvsse002-pets.esl", "type": "BOOK DESC", "editor_id": "ccVSVSSE002_Instructions_Fox", "string": "<font face='$HandwrittenFont'><div align=\"left\">\r\n<b>初種月4日</b>\r\n我想成為木精靈真的有助於讓我和動物相處！這隻小狐狸今日膽大妄為地晃到我面前，想要討東西吃。那時我身上只有甜捲可以吃，不過牠看上去很喜歡。不知道這種讓動物親近的天賦能不能在馬身上。如果可以，偷東西將變得易如反掌。\r\n\r\n<b>初種月15日</b>\r\n這個毛茸茸的討厭鬼就是不肯放過我！我到哪牠都跟著，即便是下水道也無法讓牠打退堂鼓。但至少牠很安靜，不會驚動守衛......\r\n\r\n<b>雨手月1日</b>\r\n狐狸嗅聞貴重物品的本事真的不小。光是今天早上牠就從泥裡挖出了純銀吊墜。要不是牠努力用爪子拍地，我是絕對不可能發現那個吊墜的。牠肯定是被東西的亮光吸引住了。但我可不能為此怪罪牠，真的。\r\n\r\n<b>雨手月9日</b>\r\n事實證明，發現小甜捲是我這段時間最幸運的事。牠的銳眼和直覺勝過我見過的所有小偷，就連小煩也不例外。我已經不記得上次零錢包這麼重是什麼時候的事了。現在我走起路來就和帝國軍隊一樣叮叮噹噹的。\r\n\r\n<b>雨手月25日</b>\r\n我打算嘗試馴服鎮外的狼。最近有其他小偷在盯著我。如果我腳邊有頭狼跟著，他們想動手就可得再想想了。 "}, {"original": "Pickpocket's Journal", "form_id": "05000825|ccvsvsse002-pets.esl", "type": "BOOK FULL", "editor_id": "ccVSVSSE002_Instructions_Skeever", "string": "扒手的日記"}, {"original": "<font face='$HandwrittenFont'>\r\nI ain't been having no luck at lifting gold off them rich merchants in the market, and those thieves guild s'wits don't want me. Said I'm too clumsy. I'm running out of food and skooma, may need to start killing some of the skeevers down here for meat...\r\n\r\nOne of the skooma dealers at the docks told me he knew a guy from Morrowind who trained a nix hound to distract his marks - helped him steal a heaping pile of gold, he said. Don't know if I believe a word of it, but what else am I going to do? Might as well give it a try.\r\n\r\nToo bad there ain't any nix hounds around here. Plenty of skeevers, though...I'm gonna need to catch some anyway for food, might as well have a go at training one first.", "form_id": "05000825|ccvsvsse002-pets.esl", "type": "BOOK DESC", "editor_id": "ccVSVSSE002_Instructions_Skeever", "string": "<font face='$HandwrittenFont'>\r\n我運氣不好，總是沒辦法在市場偷到有錢人的金幣。盜賊公會的聰明人也不想要我加入，他們說我太笨了。我的食物和斯庫瑪都快沒了，可能需要殺點惡鼠來吃才行......\r\n\r\n碼頭有個斯庫瑪商人告訴我，他知道有個從晨風來的人訓練了一隻尼克斯獵犬，用來轉移目標的注意力......並藉此偷到了大筆金幣。我不知道這話該不該信，但我也沒其他選擇了。不妨試試吧。\r\n\r\n只可惜這裡沒有尼克斯獵犬，惡鼠倒是不少......既然我都得抓幾隻惡鼠來吃了，不如就先拿一隻訓練看看吧。"}, {"original": "Vampire Naturalist's Journal", "form_id": "05000824|ccvsvsse002-pets.esl", "type": "BOOK FULL", "editor_id": "ccVSVSSE002_Instructions_Spider", "string": "吸血鬼實驗者的日記"}, {"original": "<font face='$HandwrittenFont'><div align=\"left\">\r\nWhile my siblings may be obsessed with the taste that fear imbues in the blood of mortals, I find that pain lends a much more potent sweetness. And nothing is quite so capable of causing pain as the venom of a frostbite spider.\r\n\r\nBut alas, the usual method of harvesting frostbite venom involves killing the spider - a costly and dangerous venture. Far too little venom can be obtained through violent methods for the experiments I wish to conduct. \r\n\r\nTo that end, I have captured a frostbite spider from deeper in the cave so that I may regularly collect its venom, which seems to be unusually potent. I discovered that it appears to have some measure of intelligence, and I have been attempting to teach it to obey certain orders. Oh how delicious it would be to have a tamed spider entangle my prey on command!\r\n\r\nI think I will name it Arachnia. ", "form_id": "05000824|ccvsvsse002-pets.esl", "type": "BOOK DESC", "editor_id": "ccVSVSSE002_Instructions_Spider", "string": "<font face='$HandwrittenFont'><div align=\"left\">\r\n我的手足沉迷於凡人深入骨髓的恐懼味道，但我發現痛苦品嘗起來才是更加甜美。而最能引起痛苦的事物，莫過於霜噬蜘蛛的毒液了。\r\n\r\n但可惜的是，採集毒液的常見方法包括殺死蜘蛛，代價高昂又危險。且透過這種暴力方法取得的毒液實在太少，不足以用來進行我想做的實驗。\r\n\r\n因此，我從洞穴深處抓來了一隻霜噬蜘蛛，以便定期採集牠的毒液。這隻蜘蛛的毒液似乎特別強大，我還發現牠有某種程度的智力。我正試圖教牠聽從某些命令。如果我能馴服蜘蛛，讓牠隨我命令纏住獵物，那該是多麼美好的事情啊！\r\n\r\n我想幫牠取名叫阿拉克尼亞。 "}, {"original": "For Sale", "form_id": "0500080A|ccvsvsse002-pets.esl", "type": "BOOK FULL", "editor_id": "ccVSVSSE002_Notice_Goat", "string": "出售"}, {"original": "<font face=\"$HandwrittenFont\">\r\n<font size=\"40\">For Sale</font>\r\n\r\nOne tamed mountain goat. Able to carry a hefty amount of weight to the Throat of the World and back again. Gives good milk. \r\n\r\nIf interested in purchasing, see Halvar in Rorikstead. \r\n\r\n- Halvar", "form_id": "0500080A|ccvsvsse002-pets.esl", "type": "BOOK DESC", "editor_id": "ccVSVSSE002_Notice_Goat", "string": "<font face=\"$HandwrittenFont\">\r\n<font size=\"40\">出售</font>\r\n\r\n一頭已經馴化的山羊。能夠背負沉重物品來回世界咽喉，還能產出優質羊奶。\r\n\r\n如有興趣購買，請到洛利克鎮找哈瓦爾。\r\n\r\n——哈瓦爾"}, {"original": "Notes on Tamed Rabbit", "form_id": "05000826|ccvsvsse002-pets.esl", "type": "BOOK FULL", "editor_id": "ccVSVSSE002_Instructions_Rabbit", "string": "有關馴化兔子的筆記"}, {"original": "<font face=\"$HandwrittenFont\" size=\"18\">\r\n<b>14th of Second Seed</b>\r\nThe rabbits here seem particularly adept at sniffing out potion ingredients. I recently found one nibbling on my thistle plants and managed to capture it. It is my hope that I will be able to train it to lead me to the most bountiful flora.\r\n\r\n<b>22nd of Second Seed</b>\r\nThe rabbit (which I have decided to name Thistle, in honor of the carefully cultivated plants it managed to destroy) is showing great promise. It has proven to be quite tame and even rather intelligent. Getting it to respond to commands, however, can be a bit of a trial. I've resorted to plying it with carrots. ", "form_id": "05000826|ccvsvsse002-pets.esl", "type": "BOOK DESC", "editor_id": "ccVSVSSE002_Instructions_Rabbit", "string": "<font face=\"$HandwrittenFont\" size=\"18\">\r\n<b>次種月14日</b>\r\n這裡的兔子似乎別擅長聞出藥水材料的位置。我最近才在自己種的薊叢發現一隻吃得津津有味的兔子，並設法逮住了牠。希望我能訓練好牠，讓牠帶我去找到珍貴的花草。\r\n\r\n<b>次種月22日</b>\r\n兔子（為紀念牠設法摧毀我精心培育的植物，我決定叫牠小薊）的表現十分可期。牠很溫馴，甚至非常聰明。不過讓牠回應我的命令算是小小的考驗，而這點我已經用源源不斷的胡蘿蔔解決了。 "}, {"original": "Animal Collar", "form_id": "05000871|ccvsvsse002-pets.esl", "type": "MISC FULL", "editor_id": "ccVSVSSE002_Collar_dummy", "string": "動物項圈"}, {"original": "Animal Pack", "form_id": "05000851|ccvsvsse002-pets.esl", "type": "MISC FULL", "editor_id": "ccVSVSSE002_Saddlebags_dummy", "string": "動物背包"}, {"original": "Hilda", "form_id": "05000830|ccvsvsse002-pets.esl", "type": "NPC_ FULL", "editor_id": "ccVSVSSE002_Goat", "string": "希爾達"}, {"original": "Thistle", "form_id": "0500082F|ccvsvsse002-pets.esl", "type": "NPC_ FULL", "editor_id": "ccVSVSSE002_Rabbit", "string": "小薊"}, {"original": "Vampire Naturalist", "form_id": "0500082E|ccvsvsse002-pets.esl", "type": "NPC_ FULL", "editor_id": "ccVSVSSE002_Vampire", "string": "吸血鬼實驗者"}, {"original": "Sweet Roll", "form_id": "05000831|ccvsvsse002-pets.esl", "type": "NPC_ FULL", "editor_id": "ccVSVSSE002_Fox", "string": "小甜捲"}, {"original": "Arachnia", "form_id": "0500082B|ccvsvsse002-pets.esl", "type": "NPC_ FULL", "editor_id": "ccVSVSSE002_Spider", "string": "阿拉克尼亞"}, {"original": "Halvar", "form_id": "0500082C|ccvsvsse002-pets.esl", "type": "NPC_ FULL", "editor_id": "ccVSVSSE002_Farmer", "string": "哈瓦爾"}, {"original": "Thief", "form_id": "0500082A|ccvsvsse002-pets.esl", "type": "NPC_ FULL", "editor_id": "ccVSVSSE002_DeadThief", "string": "盜賊"}, {"original": "Skritch", "form_id": "0500082D|ccvsvsse002-pets.esl", "type": "NPC_ FULL", "editor_id": "ccVSVSSE002_Skeever", "string": "斯克里奇"}, {"original": "Skeever Cage Key", "form_id": "05000833|ccvsvsse002-pets.esl", "type": "KEYM FULL", "editor_id": "ccVSVSSE002_CageKey_Skeever", "string": "惡鼠籠鑰匙"}, {"original": "Spider Cage Key", "form_id": "05000832|ccvsvsse002-pets.esl", "type": "KEYM FULL", "editor_id": "ccVSVSSE002_CageKey_Spider", "string": "蜘蛛籠鑰匙"}, {"original": "Potent Frostbite Venom", "form_id": "05000834|ccvsvsse002-pets.esl", "type": "ALCH FULL", "editor_id": "ccVSVSSE002_PotentFrostbiteVenom", "string": "強效霜噬毒液"}, {"original": "The Ratway", "form_id": "0003B698|Skyrim.esm", "type": "CELL FULL", "editor_id": "RiftenRatway01", "string": "鼠道"}, {"original": "Cronvangr Cave", "form_id": "000152A7|Skyrim.esm", "type": "CELL FULL", "editor_id": "CronvangrHall01", "string": "克隆萬格洞穴"}, {"original": "The Bannered Mare", "form_id": "0001605E|Skyrim.esm", "type": "CELL FULL", "editor_id": "WhiterunBanneredMare", "string": "敕旗母馬客棧"}, {"original": "Skyrim", "form_id": "0000003C|Skyrim.esm", "type": "WRLD FULL", "editor_id": "Tamriel", "string": "天際省"}, {"original": "Spin a web trap!", "form_id": "05000823|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Spider_BranchWebTopic", "string": "編織蛛網陷阱！"}, {"original": "(grunt)", "form_id": "050008DE|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "哼......"}, {"original": "Hey there, Thistle. Want a treat?", "form_id": "050008A0|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Rabbit_BranchRecruitTopic", "string": "嗨，小薊。想吃東西嗎？"}, {"original": "(squeal)", "form_id": "050008A1|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "嘰嘰......吱吱......"}, {"original": "(squeal)", "form_id": "050008A2|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "嘰嘰......吱吱......"}, {"original": "Hey buddy, are you hungry?", "form_id": "050008A2|ccvsvsse002-pets.esl", "type": "INFO RNAM", "string": "嗨，小可愛，餓了嗎？"}, {"original": "Go find an ingredient!", "form_id": "050008A3|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Rabbit_BranchFetchTopic", "string": "去找材料吧！"}, {"original": "(grunt)", "form_id": "050008A4|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "哼......"}, {"original": "Go find food!", "form_id": "050008A5|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Skeever_BranchFetchTopic", "string": "去找食物吧！"}, {"original": "(grunt)", "form_id": "050008A6|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "哼......"}, {"original": "What's that you got there?", "form_id": "050008A7|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Rabbit_BranchFoundItemTopic", "string": "你那是什麼？"}, {"original": "(squeal)", "form_id": "050008A8|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "嘰嘰......吱吱......"}, {"original": "What's that you got there?", "form_id": "050008A9|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Skeever_BranchFindItemTopic", "string": "你那是什麼？"}, {"original": "(squeal)", "form_id": "050008AA|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "嘰嘰......吱吱......"}, {"original": "Harvest venom", "form_id": "050008AB|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Spider_BranchCollectVenomTopic", "string": "採集毒液"}, {"original": "(grunt)", "form_id": "050008AC|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "哼......"}, {"original": "Let's harvest some venom.", "form_id": "050008AC|ccvsvsse002-pets.esl", "type": "INFO RNAM", "string": "來收集一些毒液吧。"}, {"original": "Hey there, Sweet Roll. Want a treat?", "form_id": "050008AD|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Fox_BranchRecruitTopic", "string": "嗨，小甜捲。想吃東西嗎？"}, {"original": "(squeal)", "form_id": "050008AE|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "嘰嘰......吱吱......"}, {"original": "(squeal)", "form_id": "050008AF|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "嘰嘰......吱吱......"}, {"original": "Hey buddy, are you hungry?", "form_id": "050008AF|ccvsvsse002-pets.esl", "type": "INFO RNAM", "string": "嗨，小可愛，餓了嗎？"}, {"original": "What's that you got there?", "form_id": "050008B0|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Fox_BranchFoundItemTopic", "string": "你那是什麼？"}, {"original": "(squeal)", "form_id": "050008B1|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "嘰嘰......吱吱......"}, {"original": "Go find something valuable!", "form_id": "050008B2|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Fox_BranchFetchTopic", "string": "去找值錢的東西吧！"}, {"original": "(grunt)", "form_id": "050008B3|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "哼......"}, {"original": "Collect milk", "form_id": "050008B4|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Goat_BranchCollectMilkTopic", "string": "擠奶"}, {"original": "(grunt)", "form_id": "050008B5|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "哼......"}, {"original": "Let's collect some milk.", "form_id": "050008B5|ccvsvsse002-pets.esl", "type": "INFO RNAM", "string": "來擠奶吧。"}, {"original": "Can I buy your goat?", "form_id": "050008B6|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_FarmerDialogueQuest_BranchBuyGoatTopic", "string": "我可以買你的羊嗎？"}, {"original": "Here's my offer...(<Global=ccVSVSSE002_GoatPrice> gold)", "form_id": "050008B8|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_FarmerDialogueQuest_AcceptTopic", "string": "這是我的出價......（<Global=ccVSVSSE002_GoatPrice>金幣）"}, {"original": "Actually, I don't have enough coin.", "form_id": "050008BA|ccvsvsse002-pets.esl", "type": "INFO RNAM", "string": "老實說，我的錢不夠。"}, {"original": "On second thought, never mind.", "form_id": "050008BB|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_FarmerDialogueQuest_RejectTopic", "string": "我想了想，還是算了。"}, {"original": "Hey there, buddy. Want to come with me?", "form_id": "050008BD|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Goat_BranchRecruitTopic", "string": "嗨，兄弟。你想跟我走嗎？"}, {"original": "(baa!)", "form_id": "050008BE|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "咩！"}, {"original": "Equip Pack", "form_id": "050008E2|ccvsvsse002-pets.esl", "type": "DIAL FULL", "editor_id": "ccVSVSSE002_PetControlQuest_Goat_BranchEquip2Topic", "string": "裝備背包"}, {"original": "(baa!)", "form_id": "050008ED|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "咩！"}, {"original": "Let's equip your pack.", "form_id": "050008ED|ccvsvsse002-pets.esl", "type": "INFO RNAM", "string": "來裝上背包吧。"}, {"original": "(grunt)", "form_id": "05000913|ccvsvsse002-pets.esl", "type": "INFO NAM1", "index": 1, "string": "哼......"}, {"original": "Let's remove your pack.", "form_id": "05000913|ccvsvsse002-pets.esl", "type": "INFO RNAM", "string": "來卸下背包吧。"}, {"original": "Equ