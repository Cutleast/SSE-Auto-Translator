[{"original": "Smite Damage", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "強打ダメージ"}, {"original": "Damages the target for <mag> points. Depletes <20> Stamina from wielder per hit.", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "対象に <mag> ポイントのダメージを与える。1回当たるごとにスタミナを <20> 減少させる。"}, {"original": "Reduced Stamina", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_DamageStamina", "string": "スタミナ低下"}, {"original": "Drains <mag> points from stamina.", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_DamageStamina", "string": "スタミナを <mag> ポイント低下させる。"}, {"original": "Absorb Health", "form_id": "03000807|ccbgssse006-stendarshammer.esl", "type": "ENCH FULL", "editor_id": "ccBGSSSE006_EnchDrainHealth", "string": "体力吸収"}, {"original": "Smite", "form_id": "0300080A|ccbgssse006-stendarshammer.esl", "type": "SPEL FULL", "editor_id": "ccBGSSSE006_DrainStamina", "string": "スタミナ低下"}, {"original": "Stendarr's Hammer", "form_id": "03000D62|ccbgssse006-stendarshammer.esl", "type": "WEAP FULL", "editor_id": "ccBGSSSE006StendarsHammer", "string": "ステンダールのハンマー"}, {"original": "Dwemer Museum", "form_id": "00030313|Skyrim.esm", "type": "CELL FULL", "editor_id": "MarkarthWizardsQuarters01", "string": "ドゥーマー博物館"}, {"original": "If I had a Hammer", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST FULL", "editor_id": "ccBGSSSE006_Quest", "string": "ハンマーがあれば"}, {"original": "Rumor has it that Stendarr's Hammer has been found deep under Markarth. They say it is in the Dwarven Museum there.", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST CNAM", "index": 95, "editor_id": "ccBGSSSE006_Quest", "string": "マルカルスの地下深くでステンダールのハンマーが見つかったという噂だ。地下のドワーフの博物館にあるらしい。"}, {"original": "I have successfully stolen Stendarr's Hammer from the Dwarven Museum in Markarth.", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST CNAM", "index": 96, "editor_id": "ccBGSSSE006_Quest", "string": "マルカルスのドワーフの博物館から、ステンダールのハンマーを盗むことに成功した。"}, {"original": "Steal Stendarr's Hammer", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST NNAM", "index": 0, "editor_id": "ccBGSSSE006_Quest", "string": "ステンダールのハンマーを盗む"}]