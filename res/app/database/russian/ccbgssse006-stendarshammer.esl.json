[{"original": "Smite Damage", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "Сокрушительный урон"}, {"original": "Damages the target for <mag> points. Depletes <20> Stamina from wielder per hit.", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "Наносит цели <mag> ед. урона. Уменьшает запас сил владельца на <20> ед. за удар."}, {"original": "Reduced Stamina", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_DamageStamina", "string": "Запас сил уменьшен"}, {"original": "Drains <mag> points from stamina.", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_DamageStamina", "string": "Отнимает <mag> ед. запаса сил."}, {"original": "Absorb Health", "form_id": "03000807|ccbgssse006-stendarshammer.esl", "type": "ENCH FULL", "editor_id": "ccBGSSSE006_EnchDrainHealth", "string": "Поглощение здоровья"}, {"original": "Smite", "form_id": "0300080A|ccbgssse006-stendarshammer.esl", "type": "SPEL FULL", "editor_id": "ccBGSSSE006_DrainStamina", "string": "Высасывание запаса сил"}, {"original": "Stendarr's Hammer", "form_id": "03000D62|ccbgssse006-stendarshammer.esl", "type": "WEAP FULL", "editor_id": "ccBGSSSE006StendarsHammer", "string": "Молот Стендарра"}, {"original": "Dwemer Museum", "form_id": "00030313|Skyrim.esm", "type": "CELL FULL", "editor_id": "MarkarthWizardsQuarters01", "string": "Музей двемеров"}, {"original": "If I had a Hammer", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST FULL", "editor_id": "ccBGSSSE006_Quest", "string": "Был бы у меня Молот"}, {"original": "Rumor has it that Stendarr's Hammer has been found deep under Markarth. They say it is in the Dwarven Museum there.", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST CNAM", "index": 95, "editor_id": "ccBGSSSE006_Quest", "string": "Говорят, где-то в подземельях под Маркартом нашли Молот Стендарра. По слухам, его отправили в музей двемеров."}, {"original": "I have successfully stolen Stendarr's Hammer from the Dwarven Museum in Markarth.", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST CNAM", "index": 96, "editor_id": "ccBGSSSE006_Quest", "string": "Я сумел похитить Молот Стендарра из музея двемеров в Маркарте."}, {"original": "Steal Stendarr's Hammer", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST NNAM", "index": 0, "editor_id": "ccBGSSSE006_Quest", "string": "Украсть Молот Стендарра"}]