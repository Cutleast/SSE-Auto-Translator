[{"original": "Smite Damage", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "Daño por castigo"}, {"original": "Damages the target for <mag> points. Depletes <20> Stamina from wielder per hit.", "form_id": "03000808|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_EnchDamageFFContact", "string": "Inflige un daño de <mag> puntos al objetivo. El portador pierde <20> de aguante por impacto."}, {"original": "Reduced Stamina", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF FULL", "editor_id": "ccBGSSSE006_DamageStamina", "string": "Aguante reducido"}, {"original": "Drains <mag> points from stamina.", "form_id": "03000809|ccbgssse006-stendarshammer.esl", "type": "MGEF DNAM", "editor_id": "ccBGSSSE006_DamageStamina", "string": "Consume <mag> puntos de aguante."}, {"original": "Absorb Health", "form_id": "03000807|ccbgssse006-stendarshammer.esl", "type": "ENCH FULL", "editor_id": "ccBGSSSE006_EnchDrainHealth", "string": "Absorber salud"}, {"original": "Smite", "form_id": "0300080A|ccbgssse006-stendarshammer.esl", "type": "SPEL FULL", "editor_id": "ccBGSSSE006_DrainStamina", "string": "Absorción de aguante"}, {"original": "Stendarr's Hammer", "form_id": "03000D62|ccbgssse006-stendarshammer.esl", "type": "WEAP FULL", "editor_id": "ccBGSSSE006StendarsHammer", "string": "Martillo de Stendarr"}, {"original": "Dwemer Museum", "form_id": "00030313|Skyrim.esm", "type": "CELL FULL", "editor_id": "MarkarthWizardsQuarters01", "string": "Museo dwemer"}, {"original": "If I had a Hammer", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST FULL", "editor_id": "ccBGSSSE006_Quest", "string": "Si tuviera un martillo..."}, {"original": "Rumor has it that Stendarr's Hammer has been found deep under Markarth. They say it is in the Dwarven Museum there.", "form_id": "03000805|ccbgssse006-stendarshammer.esl", "type": "QUST CNAM", "index": 95, "